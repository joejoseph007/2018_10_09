FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      S;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0;
    }

    AMI2
    {
        type            cyclicAMI;
        value           uniform 0;
    }

    inlet 
    {
        type            fixedValue;
        value           uniform 0;
    }

    outlet 
    {
        type            zeroGradient;
        value           uniform 0;
    }

    top 
    {
        type            zeroGradient;
        value           uniform 0;
    }

    bottom 
    {
        type            zeroGradient;
        value           uniform 0;
    }

    "(VAWT1|VAWT2|VAWT3|SHAFT)"
    {
        type            zeroGradient;
        value           uniform 0;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}

